  <div class="container-fluid py-4">
    <div class="row">
      <div class="col-12">
        <div class="card mb-4">
          <div class="card-header pb-0 d-flex justify-content-lg-between">
            <h3>Danh sách các nhà xe hiện có</h3>
            <a href="/admin/NhaXe/CreateNX" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Thêm nhà xe</a>
          </div>
          <div class="card-body px-0 pt-0 pb-2">
            <div class="table-responsive p-0">
              <table class="table align-items-center mb-0">
                <thead>
                  <tr>
                    <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Tên nhà xe</th>
                    <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Địa chỉ nhà xe</th>
                    <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Số điện thoại
                    </th>
                    <th class="text-secondary opacity-7"></th>
                  </tr>
                </thead>
                <tbody>
                  {{#each nhaXe}}
                  <tr id={{_id}}>
                    <td>
                      <div class="d-flex px-2 py-1">
                        <h6 class="mb-0 text-sm">{{TenNhaXe}}</h6>
                      </div>
                    </td>
                    <td>
                      <p class="text-xs font-weight-bold mb-0">{{DiaChiNhaXe}}</p>  
                    </td>
                    <td class="align-middle text-center text-sm">
                      <p class="text-xs font-weight-bold mb-0">{{SoDT}}</p>  
                    </td>
                    <td class="align-middle text-center">
                      <span class="text-secondary text-xs font-weight-bold">{{DanhGia}}</span>
                    </td>
                    <td class="align-middle">
                      <a href="/admin/NhaXe/{{_id}}" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip"
                        data-original-title="Edit">
                        Chỉnh sửa
                      </a>
                    </td>
                    <td class="align-middle">
                      <a href="/admin/NhaXe/delete/{{_id}}" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip"
                        data-original-title="Delete" data-confirm="Are you sure to delete this item?">
                        Xóa
                      </a>
                    </td>
                  </tr>
                  {{/each}}

                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
